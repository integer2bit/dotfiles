{
  "name": "friendly-snippets",
  "engines": {
    "vscode": "^1.11.0"
  },
  "contributes": {
    "snippets": [
      {
        "language": [
          "all"
        ],
        "path": "global.json"
      },
      {
        "language": "lua",
        "path": "./lua.json"
      },
      {
        "language": [
          "gitcommit"
        ],
        "path": "./gitcommit.json"
      },
      {
        "language": [
          "markdown",
          "rmd"
        ],
        "path": "./markdown.json"
      }
    ]
  }
}
